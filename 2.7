using System;
using System.Collections.Generic;
using System.Linq;

namespace MostFrequentNumber
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Enter the array of integers (space separated on a single line):");
            int[] inputArray = Console.ReadLine().Split(' ').Select(int.Parse).ToArray();

            Dictionary<int, int> frequencyDictionary = new Dictionary<int, int>();

            int maxFrequency = 0;
            int mostFrequentNumber = inputArray[0];

            foreach (int number in inputArray)
            {
                if (frequencyDictionary.ContainsKey(number))
                {
                    frequencyDictionary[number]++;
                }
                else
                {
                    frequencyDictionary[number] = 1;
                }

                if (frequencyDictionary[number] > maxFrequency)
                {
                    maxFrequency = frequencyDictionary[number];
                    mostFrequentNumber = number;
                }
            }

            Console.WriteLine($"The number {mostFrequentNumber} is the most frequent (occurs {maxFrequency} times)");
        }
    }
}
